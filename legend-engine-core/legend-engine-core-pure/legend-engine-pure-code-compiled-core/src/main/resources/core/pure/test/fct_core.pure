import meta::core::runtime::*;
import meta::pure::mapping::*;
import meta::pure::test::fct::*;

Profile meta::pure::test::fct::FCT
{
    stereotypes : [test, adapter];
    tags: [grammarDoc, grammarCharacters, adapterName];
}


function
    <<FCT.adapter>>
    {
        FCT.adapterName='execute'
    }
   meta::pure::test::testWrapper::executeWrapper<T|y>(f:FunctionDefinition<{->T[y]}>[1], m:Mapping[1], runtime:Runtime[1], extensions:meta::pure::extension::Extension[*]):meta::pure::test::fct::ExecuteResult<T>[1]
{
  ^ExecuteResult<T>(result=meta::pure::router::execute($f,$m,$runtime,$extensions));
}

